{
  "files.exclude": {
    "**/*.dyn_hi": true,
    "**/*.dyn_o": true,
    "**/*.hi": true,
    "**/*.o": true,
    "**/*.o-boot": true,
    "**/*.hi-boot": true,
    "**/dist-newstyle": true,
    "**/.stack-work": true,
    "**/.ghc.environment.*": true,
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [
    80,
    90
  ],
  "editor.tabSize": 4
}
